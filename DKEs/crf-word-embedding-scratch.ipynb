    "from gensim.models import KeyedVectors\n",
    "import numpy as np\n",
    "# Load vectors directly from the file\n",
    "model1 = KeyedVectors.load_word2vec_format('GoogleNews-vectors-negative300.bin', binary=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
